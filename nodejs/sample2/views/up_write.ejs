<%-include('header.ejs') %>

<!-- Begin page content -->
<main role="main" class="flex-shrink-0">
    <div class="container">
      <section>
          <div class="row">
              <div class="col-md-12">
                  <div class="box box-primary">
                      <div class="box-header with-border">
                          <h3 class="box-title mt-5">상품등록</h3>
                      </div>
                      <!-- 절대경로 /board/register -->
                      <form role="form" method="post" action="./up_writePro" enctype="multipart/form-data">
